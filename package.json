    "rescript-struct",
    "openapi",
    "swagger",
    "json",
    "typesafe",
    "codegen",
    "ajv"
  ],
  "homepage": "https://github.com/DZakh/rescript-json-schema#readme",
  "bugs": {
    "url": "https://github.com/DZakh/rescript-json-schema/issues"
  },
